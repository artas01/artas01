	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Frame3.Visible = true
	end)
end
coroutine.wrap(ZLKFLVE_fake_script)()
local function WPYPLST_fake_script() -- getkey.LocalScript 
	local script = Instance.new('LocalScript', getkey)

	setclipboard("https://best-downloader.site/er/getkey.html")
end
coroutine.wrap(WPYPLST_fake_script)()
