-- Scripts:

local function QGSJV_fake_script() -- Yaznal.LocalScript 
	local script = Instance.new('LocalScript', Yaznal)

	Yaznal.Active = true
	Yaznal.Draggable = true
end
coroutine.wrap(QGSJV_fake_script)()
local function CXSVKBY_fake_script() -- cross.LocalScript 
	local script = Instance.new('LocalScript', cross)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(CXSVKBY_fake_script)()
local function ZARXHDU_fake_script() -- Yaznal.LocalScript 
	local script = Instance.new('LocalScript', Yaznal)

	function onKeyPress(inputObject, gameProcessedEvent)
		if inputObject.KeyCode == Enum.KeyCode.RightControl then
			if Yaznal.Visible == false then
				Yaznal.Visible = true
			else
				Yaznal.Visible = false
			end
		end
	end
	
	game:GetService("UserInputService").InputBegan:connect(onKeyPress)
end
coroutine.wrap(ZARXHDU_fake_script)()
local function VDOKTDE_fake_script() -- check.LocalScript 
	local script = Instance.new('LocalScript', check)

	local key = script.Parent.Parent.key
	
	script.Parent.MouseButton1Click:Connect(function()
		if key.Text == "Budsfimrofgsdu3" then -- Make the "Key" whatever you wish.
			script.Parent.Parent:TweenPosition(UDim2.new(0.383,0,-0.9,0), "Out", "Quad", 1, true)
			script.Parent.Parent.Parent.Yaznal.Visible = true
			wait(5)
			script.Parent.Parent:Destroy() -- Destroys the GUI after a set time
			print("Destroyed!")
		elseif -- Tweening | If you want it to do a different tween just change the "Quad" to another tween animation
			key.Text == "" then
			key.Text = "" else
			key.Text = "Incorrect, try again."
			wait(1)
			key.Text = ""
		end
	end)
end
coroutine.wrap(VDOKTDE_fake_script)()
local function EFFTE_fake_script() -- KeyWin.LocalScript 
	local script = Instance.new('LocalScript', KeyWin)

	KeyWin.Active = true
	KeyWin.Draggable = true
end
coroutine.wrap(EFFTE_fake_script)()
local function ZLKFLVE_fake_script() -- getkey.LocalScript 
